 year = {2010},
 isbn = {978-1-4503-0006-3},
 location = {Milwaukee, Wisconsin, USA},
 pages = {331--335},
 numpages = {5},
 url = {http://doi.acm.org/10.1145/1734263.1734379},
 doi = {10.1145/1734263.1734379},
 acmid = {1734379},
 publisher = {ACM},
 address = {New York, NY, USA},
 keywords = {cs education, learning, object-oriented concepts, peer review},
 abstract = {In computer science, students could benefit from exposure to critical programming concepts from multiple perspectives. Peer review is one method to allow students to experience authentic uses of the concepts in a non-programming manner. In this work, we examine the use of the peer review process in early, object-oriented, computer science courses as a way to develop the reviewers' knowledge of object-oriented programming concepts, specifically Abstraction, Decomposition, and Encapsulation.

To study these ideas, we used peer review exercises in two CS2 classes at local universities over the course of a semester. Using three groups (one reviewing their peers, one reviewing the instructor, and one completing small design or coding exercises), we measured the students' conceptual understanding throughout the semester with concept maps and the reviews they completed. We found that reviewing helped students learn Decomposition, especially those reviewing the instructor's programs. Overall, peer reviews are a valuable method for teaching Decomposition to CS2 students and can be used as an alternative way to learn object-oriented programming concepts.}
}

@inproceedings{Waite:2016:ACC:2978249.2978272,
 author = {Waite, Jane and Curzon, Paul and Marsh, William and Sentance, Sue},
 title = {Abstraction and Common Classroom Activities},
 booktitle = {Proceedings of the 11th Workshop in Primary and Secondary Computing Education},
 series = {WiPSCE '16},
 year = {2016},
 isbn = {978-1-4503-4223-0},
 location = {M\&\#252;nster, Germany},
 pages = {112--113},
