 acmid = {506600},
 publisher = {ACM},
 address = {New York, NY, USA},
 keywords = {concept map, handhelds, learner centered design, scaffolds},
 abstract = {Handheld computers offer the flexibility and mobility to be "ready at hand" tools that can facilitate learning anytime, anywhere. Applying the principles of Learner Centered Design [2], we have developed Pocket PiCoMap to support students engaged in complex concept mapping activities using handheld computers. Pocket PiCoMap uses scaffolds to address specific student needs; for instance, a color scaffold was provided to address students' difficulty organizing and understanding information displayed on small screens. Pocket PiCoMap was piloted for six weeks with 33 eighth grade students in mid-Michigan classrooms, and our preliminary results suggest that scaffolds are both useful and viable for handheld educational software.}
}

@inproceedings{Portmann:2012:DSH:2389656.2389658,
 author = {Portmann, Edy and Kaufmann, Michael Alexander and Graf, C{\'e}dric},
 title = {A Distributed, Semiotic-inductive, and Human-oriented Approach to Web-scale Knowledge Retrieval},
 booktitle = {Proceedings of the 2012 International Workshop on Web-scale Knowledge Representation, Retrieval and Reasoning},
 series = {Web-KR '12},
 year = {2012},
 isbn = {978-1-4503-1711-5},
 location = {Maui, Hawaii, USA},
 pages = {1--8},
 numpages = {8},
 url = {http://doi.acm.org/10.1145/2389656.2389658},
 doi = {10.1145/2389656.2389658},
 acmid = {2389658},
 publisher = {ACM},
 address = {New York, NY, USA},
 keywords = {computational semiotics, distributed information retrieval, explorative search, human-computer information retrieval, human-oriented computer interaction, inductive inference, knowledge discovery from data, web-scale knowledge retrieval},
 abstract = {Web-scale knowledge retrieval can be enabled by distributed information retrieval; clustering Web clients to a large-scale computing infrastructure for knowledge discovery from Web documents. Based on this infrastructure, we propose to apply semiotic (i.e., sub-syntactical) and inductive (i.e., probabilistic) methods for inferring concept associations in human knowledge. These associations can be combined to form a fuzzy (i.e., gradual) semantic net representing a map of the knowledge in the Web. Thus, we propose to provide interactive visualizations of these cognitive concept maps to end users, who can browse and search the Web in a human-oriented, visual, and associative interface.}
}

@inproceedings{Rigby:2009:ECR:1631728.1631801,
 author = {Rigby, Steven and Dark, Melissa J. and Ekstrom, Joseph J. and Rogers, Marcus},
 title = {The Efficacy of Cross-discipline Representations for Ill-defined Concepts},
 booktitle = {Proceedings of the 10th ACM Conference on SIG-information Technology Education},
 series = {SIGITE '09},
 year = {2009},
 isbn = {978-1-60558-765-3},
 location = {Fairfax, Virginia, USA},
 pages = {229--234},
 numpages = {6},
 url = {http://doi.acm.org/10.1145/1631728.1631801},
 doi = {10.1145/1631728.1631801},
 acmid = {1631801},
 publisher = {ACM},
 address = {New York, NY, USA},
 keywords = {ias education},
 abstract = {A universal problem to our society is the dramatic increase in the number of security threats, risks, and vulnerabilities to our nation's computer systems, data, and infrastructure. Our future success depends upon the problem-solving and thinking abilities of professionals entering the Information Technology field. These professionals will be faced with many problems that are complex, Ill-defined, and multi-disciplinary in nature. But how do we, as educators, prepare these professionals to be successful? Are traditional approaches sufficient for the complexity they will face? This study examines ways of increasing learners' expertise of ill-defined concepts through the use of varying types of representations. Through a quasi-experimental research methodology, students were assigned into one of three groups and given multiple representations of an ill-defined concept "threat analysis". After each treatment, students created concept maps of their understanding of threat analysis, which was then scored against an expert concept map. Results show that multiple representations did increase conceptual understanding of an ill-defined concept from the first treatment to the second with diminishing returns thereafter. The varying contexts of the representations was not a factor, however the use of different instructional strategies did show a difference over the three treatments. Future research in examining how the structuredness of the representations presented to the learner increases conceptual understanding of ill-defined concepts will be of benefit to the field.}
}

@inproceedings{Choi:2009:RAC:1655925.1656187,
 author = {Choi, K. H.},
 title = {A Research Analysis on the Concept of Converging Technology and Converging Types of Information Technology},
 booktitle = {Proceedings of the 2Nd International Conference on Interaction Sciences: Information Technology, Culture and Human},
 series = {ICIS '09},
 year = {2009},
 isbn = {978-1-60558-710-3},
 location = {Seoul, Korea},
 pages = {1422--1426},
 numpages = {5},
 url = {http://doi.acm.org/10.1145/1655925.1656187},
 doi = {10.1145/1655925.1656187},
 acmid = {1656187},
 publisher = {ACM},
 address = {New York, NY, USA},
 keywords = {converging technology roadmap, converging theory, converging type},
 abstract = {The future technological revolution is expected to be led by converging technologies, having enormous effects on economic and industrial environments in the future. But there have been few attempts to make empirical approach to the concept of converging technologies.

In order to obtain the overall concept of converging technologies, this study will look into the field of information & communication as a subject area. The scope of the study is especially limited to the information & computation technologies which have been listed in the National R&D Plan. There are three main sub-plans in the National R&D Plan; Green Growth, New Growth Engine, and Science & Technology Basic Plan. To analyze the concept of converging technologies, this study selected the core converging technologies from each area to analyze the concept of converging technologies.

Then, their stem technologies are analyzed and presented in a concept map to show how the core converging technologies are created from them. Also, the core technologies are presented on the concept map to distinguish the converging types based on them.

This study aims to empirically present the concept of converging technology and to make clearly each converging types based on their core converging technologies.}
}

@inproceedings{Davis:2003:IUI:604045.604054,
 author = {Davis, J. and Leelawong, K. and Belynne, K. and Bodenheimer, B. and Biswas, G. and Vye, N. and Bransford, J.},
 title = {Intelligent User Interface Design for Teachable Agent Systems},
 booktitle = {Proceedings of the 8th International Conference on Intelligent User Interfaces},
 series = {IUI '03},
 year = {2003},
 isbn = {1-58113-586-6},
 location = {Miami, Florida, USA},
 pages = {26--33},
 numpages = {8},
 url = {http://doi.acm.org/10.1145/604045.604054},
 doi = {10.1145/604045.604054},
 acmid = {604054},
 publisher = {ACM},
 address = {New York, NY, USA},
 keywords = {active learning, feedback, learning by teaching, teachable agents},
 abstract = {This paper describes the interface components for a system called Bettys Brain, an intelligent agent we have developed for studying the learning by teaching paradigm. Our previous studies have shown that students gain better understanding of domain knowledge when they prepare to teach others versus when they prepare to take an exam. This finding has motivated us to develop computer agents that students teach using concept map representations with a visual interface. Betty is intelligent not because she learns on her own, but because she can apply qualitative-reasoning techniques to answer questions that are directly related to what she has been taught through the concept map. We evaluate the agents interfaces in terms of how well they support learning activities, using examples of their use by fifth grade students in an extensive study that we performed in a Nashville public school. A critical analysis of the outcome of our studies has led us to propose the next generation interfaces in a multi-agent paradigm that should be more effective in promoting constructivist learning and self-regulation in the learning by teaching framework}
}

@inproceedings{Turner:2010:PRC:1734263.1734379,
 author = {Turner, Scott and P{\'e}rez-Qui\~{n}ones, Manuel A. and Edwards, Stephen and Chase, Joseph},
 title = {Peer Review in CS2: Conceptual Learning},
 booktitle = {Proceedings of the 41st ACM Technical Symposium on Computer Science Education},
