 title = {FISCMAP: A Fuzzy Logic-based Quality of Concept Mapping Modelling Approach Fostering Reflective Feedback},
 booktitle = {Proceedings of the 7th International Conference on Software Development and Technologies for Enhancing Accessibility and Fighting Info-exclusion},
 series = {DSAI 2016},
 year = {2016},
 isbn = {978-1-4503-4748-8},
 location = {Vila Real, Portugal},
 pages = {293--300},
 numpages = {8},
 url = {http://doi.acm.org/10.1145/3019943.3019986},
 doi = {10.1145/3019943.3019986},
 acmid = {3019986},
 publisher = {ACM},
 address = {New York, NY, USA},
 keywords = {B-learning, CmapTool, Computer-based Concept Mapping, Fuzzy logic, Quality of Concept Map, Reflective feedback},
 abstract = {This study introduces a new model, namely FISCMAP, that explores the fuzzy logic constructs within a computer-based concept mapping environment, involving modeling techniques as vehicles to improve the intelligence of an online learning environment. From this perspective, eight CmapTool measurements are considered to form inputs to a five-level fuzzy inference system equipped with 115 expert's fuzzy rules. The CmapTool data were drawn from a b-learning environment related to a Master's course offered by a Higher Education Institution, involving 20 Master's students. Experimental results have shown that the use of the proposed FISCMAP scheme for the evaluation of user's Quality of Concept Map (QoCM), by considering constructive CM variables (metrics), can increase the accuracy and validity of the intelligent system under consideration.}
}

@inproceedings{Armendariz:2014:OOC:2556325.2567849,
 author = {Armendariz, Daniel and MacHardy, Zachary and Garcia, Daniel D.},
 title = {OCTAL: Online Course Tool for Adaptive Learning},
 booktitle = {Proceedings of the First ACM Conference on Learning @ Scale Conference},
 series = {L@S '14},
 year = {2014},
 isbn = {978-1-4503-2669-8},
 location = {Atlanta, Georgia, USA},
 pages = {141--142},
 numpages = {2},
 url = {http://doi.acm.org/10.1145/2556325.2567849},
 doi = {10.1145/2556325.2567849},
 acmid = {2567849},
 publisher = {ACM},
 address = {New York, NY, USA},
 keywords = {adaptive assessment, concept map, non-linear courses, student modeling},
 abstract = {The Online Course Tool for Adaptive Learning (OCTAL) is an adaptive exercise system that customizes the progression of question topics to each student. By creating a concept dependency graph of topics in a course and modeling a student's knowledge state, the tool presents questions that test knowledge within a student's zone of proximal development. We intend OCTAL to be a formative assessment tool that is not tied to any specific course by providing language-agnostic questions on computer science concepts. While the tool will be generalizable for many courses, our first prototype includes a concept map and question set for UC Berkeley's introductory computer science course, CS10: The Beauty and Joy of Computing. Using the tool, we will launch an experiment in the spring to investigate metacognitive improvements in the identification of knowledge gaps by presenting online course material in a nonlinear fashion.}
}

@inproceedings{Luchini:2002:SSD:506443.506600,
 author = {Luchini, Kathleen and Quintana, Chris and Krajcik, Joe and Farah, Chris and Nandihalli, Nayan and Reese, Kyle and Wieczorek, Adam and Soloway, Elliot},
